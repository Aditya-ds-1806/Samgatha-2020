            <% } %>
        </div>
        <h2 class="mt-5">PUBLICITY TEAM</h2>
        <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 row-cols-lg-4 mt-5">
            <% for(let i = 0; i < team.publicity.length; i++) { %>
            <div class="col mb-4">
                <%- include('./partials/card-II.ejs', {i: i, route: route, sponsor: team.publicity}) %>
            </div>
            <% } %>
        </div>
        <h2 class="mt-5">DECOR TEAM</h2>
        <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 row-cols-lg-4 mt-5">
            <% for(let i = 0; i < team.decor.length; i++) { %>
            <div class="col mb-4">
                <%- include('./partials/card-II.ejs', {i: i, route: route, sponsor: team.decor}) %>
            </div>
            <% } %>
        </div>
    </div>
</section>
<%- include('./partials/footer') %>