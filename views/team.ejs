<%- include('./partials/header') %>
<section>
    <%- include('./partials/navbar') %>
    <div class="container mx-auto text-center text-white mt-5">
        <h1>MEET THE TEAM</h1>
        <% for(i = 0; i < team.length; i++) { %>
        <h2 class="mt-5 border-bottom pb-3"><%= Object.keys(team[i])[0] %></h2>
        <div class="row justify-content-center row-cols-1 row-cols-sm-2 row-cols-md-3 row-cols-lg-4 mt-5">
            <% for(let j = 0; j < team[i][Object.keys(team[i])[0]].length; j++) { %>
            <div class="col mb-4">
                <%- include('./partials/card-II.ejs', { i: j, route: route, sponsor: team[i][Object.keys(team[i])[0]] }) %>
            </div>
            <% } %>
        </div>
        <% } %>
    </div>
</section>
<%- include('./partials/footer') %>