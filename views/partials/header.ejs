<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <!-- Bootstrap v4.4.1-->
    <link rel="stylesheet" href="/stylesheets/bootstrap.min.css">
    <!-- Montserrat -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,500,700,900&display=swap" rel="stylesheet">
    <!-- Blackout-sunrise -->
    <link href="/fonts/stylesheet.css" rel="stylesheet">
    <!-- Website Theme -->
    <link rel="stylesheet" href="/stylesheets/theme.css">

    <% if (route === 'events') { %>
    <!-- Overlay-Scrollbar -->
    <link rel="stylesheet" href="/stylesheets/OverlayScrollbars.min.css">
    <link rel="stylesheet" href="/stylesheets/os-theme-round-light.css">
    <link rel="stylesheet" href="/stylesheets/os-theme-block-light.css">
    <link rel="stylesheet" href="/stylesheets/events.css">
    <% } %>

    <% if (route !== 'index' && route !== 'hospitality') { %>
    <link rel="stylesheet" href="/stylesheets/cards.css">
    <% } %>

    <% if (route === 'club') { %>
    <link rel="stylesheet" href="/stylesheets/club.css">
    <% } %>

    <!-- fontawesome v5.9.0 -->
    <% if (route !== 'hospitality' && route !== 'sponsors' && route !== 'team') { %>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.9.0/css/all.css"
        integrity="sha256-PF6MatZtiJ8/c9O9HQ8uSUXr++R9KBYu4gbNG5511WE=" crossorigin="anonymous" />
    <% } %>

    <% switch (route) {
    case 'index': %>

    <link rel="stylesheet" href="/stylesheets/index.css">
    <% break; %>
    <% } %>
    <title>Samgatha</title>
    <style>
        @media (max-width: 576px) {
            * {
                font-size: 3.5vw;
            }

            span.navbar-toggler-icon {
                font-size: 20px;
            }

            a.nav-link {
                font-size: 16px !important;
            }
        }
    </style>
</head>

